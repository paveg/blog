{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export && next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "eslint --cache --ext .ts,.tsx --quiet src",
    "lint:fix": "eslint --fix --cache --ext .ts,.tsx --quiet src",
    "prettier": "prettier --write --cache -c src",
    "prepare": "husky install",
    "compile": "npx tsc"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn run lint:fix",
      "yarn run prettier",
      "bash -c 'yarn run compile'"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/serialize": "^1.1.1",
    "@emotion/server": "^11.10.0",
    "@emotion/utils": "^1.2.0",
    "@mantine/core": "^5.9.5",
    "@mantine/hooks": "^5.9.5",
    "@mantine/next": "^5.9.5",
    "@mantine/nprogress": "^5.9.5",
    "@mantine/prism": "^5.9.5",
    "@tabler/icons": "^1.118.0",
    "@vercel/analytics": "^0.1.6",
    "microcms-js-sdk": "^2.3.2",
    "next": "latest",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.43",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-parse": "^8.0.4",
    "rehype-remark": "^9.1.2",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-stringify": "^10.0.2",
    "unified": "^10.1.2",
    "zenn-content-css": "^0.1.134",
    "zenn-embed-elements": "^0.1.134",
    "zenn-markdown-html": "^0.1.134"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.12",
    "@types/node": "18.11.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gitmoji-cli": "^7.0.3",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.18",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "*"
  },
  "packageManager": "yarn@3.2.4"
}
